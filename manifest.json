{
  "manifest_version": 2,
  "name": "Campus-Dual Calendar Export",
  "description": "A chrome extension to export the calendar from Campus-Dual.",
  "version": "1.0",
  "icons": { "48": "icon48.png" },
  "content_scripts": [{
    "matches": ["https://selfservice.campus-dual.de/room/index"],
    "js": ["content.js"],
    "css": ["content.css"],
    "run_at": "document_end"
  }],
  "permissions": [
    "downloads",
    "activeTab",
    "https://selfservice.campus-dual.de/room/index"
  ]
}
